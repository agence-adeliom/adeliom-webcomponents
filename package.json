{
  "name": "@agence-adeliom/awc",
  "description": "A forward-thinking library of web components.",
  "version": "1.1.0",
  "homepage": "https://github.com/agence-adeliom/awc",
  "author": "Agence Adeliom",
  "license": "MIT",
  "customElements": "./dist/custom-elements.json",
  "web-types": "./web-types.json",
  "type": "module",
  "types": "./dist/awc.d.ts",
  "module": "./dist/awc.js",
  "main": "./dist/awc.js",
  "jsdelivr": "./dist/awc.js",
  "exports": {
    ".": {
      "types": "./dist/awc.d.ts",
      "import": "./dist/awc.js"
    },
    "./custom-elements.json": "./dist/custom-elements.json",
    "./themes/*": "./dist/themes/*",
    "./components/*": "./dist/components/*",
    "./utilities/*": "./dist/utilities/*",
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    },
    "./react/*": "./dist/react/*",
    "./translations/*": "./dist/translations/*",
    "./tailwind": {
      "import": "./dist/tailwind/index.js"
    }
  },
  "files": ["dist"],
  "keywords": ["web components", "custom elements", "components"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agence-adeliom/awc.git"
  },
  "bugs": {
    "url": "https://github.com/agence-adeliom/awc/issues"
  },
  "scripts": {
    "create": "plop --plopfile scripts/plop/plopfile.js",
    "start": "storybook dev -p 6006",
    "build": "vite build",
    "esbuild": "npm run hook:pre:build && node esbuild.config.mjs",
    "esbuild:watch": "npm run esbuild -- --watch",
    "verify": "npm run prettier:check && npm run lint:check",
    "fix": "npm run prettier:fix && npm run lint:fix",
    "prettier:check": "prettier --check --log-level=warn .",
    "prettier:fix": "prettier --write --log-level=warn .",
    "lint:check": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --max-warnings 0 --fix",
    "test": "web-test-runner --group default",
    "test:watch": "web-test-runner -- --watch --group default",
    "test:component": "web-test-runner -- --watch --group",
    "spell:check": "cspell \"**/*.{js,ts,json,html,css,md}\" --no-progress",
    "ts:check": "tsc --noEmit --project ./tsconfig.json",
    "ts:build": "tsc --project ./tsconfig.prod.json --outdir dist",
    "dependencies:list": "npm-check-updates --format repo --peer",
    "dependencies:update": "npm-check-updates --peer -u && npm install",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "prepublishOnly": "npm run build && npm run storybook:build"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.0.2",
    "@extractus/oembed-extractor": "^4.0.1",
    "@floating-ui/dom": "^1.5.3",
    "@lit/react": "^1.0.0",
    "@shoelace-style/animations": "^1.1.0",
    "@shoelace-style/localize": "^3.1.2",
    "composed-offset-position": "^0.0.4",
    "lit": "^3.0.0",
    "qr-creator": "^1.0.0",
    "unpic": "^3.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@custom-elements-manifest/analyzer": "^0.9.0",
    "@open-wc/testing": "^3.2.0",
    "@prettier/sync": "^0.3.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-replace": "^5.0.4",
    "@storybook/addon-a11y": "7.5.1",
    "@storybook/addon-docs": "7.5.1",
    "@storybook/addon-essentials": "7.5.1",
    "@storybook/addon-interactions": "7.5.1",
    "@storybook/addon-links": "7.5.1",
    "@storybook/addon-mdx-gfm": "7.5.1",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "7.5.1",
    "@storybook/theming": "7.5.1",
    "@storybook/web-components": "7.5.1",
    "@storybook/web-components-vite": "7.5.1",
    "@types/mocha": "^10.0.3",
    "@types/react": "^18.2.31",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@web/dev-server-esbuild": "^0.4.3",
    "@web/test-runner": "^0.17.2",
    "@web/test-runner-commands": "^0.8.2",
    "@web/test-runner-playwright": "^0.10.2",
    "bootstrap-icons": "^1.11.1",
    "change-case": "^5.1.2",
    "command-line-args": "^5.2.1",
    "comment-parser": "^1.4.0",
    "cspell": "^7.3.8",
    "custom-element-jet-brains-integration": "^1.3.1",
    "custom-element-vs-code-integration": "^1.2.1",
    "esbuild": "^0.19.5",
    "escape-string-regexp": "^5.0.0",
    "eslint": "^8.52.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-lit": "^1.10.1",
    "eslint-plugin-lit-a11y": "^4.1.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-wc": "^2.0.4",
    "esthetic": "^0.6.4-beta.1",
    "globby": "^13.2.2",
    "husky": "^8.0.3",
    "js-beautify": "^1.14.9",
    "lint-staged": "^15.0.2",
    "npm-check-updates": "^16.14.6",
    "plop": "^4.0.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^4.0.0",
    "remark-github-beta-blockquote-admonitions": "^2.1.2",
    "sinon": "^17.0.0",
    "storybook": "7.5.1",
    "storybook-source-link": "^2.0.9",
    "strip-css-comments": "^5.0.0",
    "terser": "^5.22.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unist-util-visit": "^5.0.0",
    "user-agent-data-types": "^0.4.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.1"
  },
  "peerDependencies": {
    "@lit-labs/react": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "*.{ts,js}": ["eslint --max-warnings 0 --cache --fix", "prettier --write"]
  }
}
