{
  "name": "@agence-adeliom/awc",
  "description": "A forward-thinking library of web components.",
  "version": "1.6.8",
  "homepage": "https://github.com/agence-adeliom/adeliom-webcomponents",
  "author": "Agence Adeliom",
  "license": "MIT",
  "customElements": "dist/custom-elements.json",
  "web-types": "./dist/web-types.json",
  "type": "module",
  "types": "dist/awc.d.ts",
  "jsdelivr": "./cdn/awc-autoloader.js",
  "exports": {
    ".": {
      "types": "./dist/awc.d.ts",
      "import": "./dist/awc.js"
    },
    "./dist/tailwind": "./dist/tailwind.cjs",
    "./dist/custom-elements.json": "./dist/custom-elements.json",
    "./dist/awc.js": "./dist/awc.js",
    "./dist/awc-autoloader.js": "./dist/awc-autoloader.js",
    "./dist/themes/*": "./dist/themes/*",
    "./dist/components/*": "./dist/components/*",
    "./dist/utilities/*": "./dist/utilities/*",
    "./dist/react": "./dist/react/index.js",
    "./dist/react/*": "./dist/react/*",
    "./dist/translations/*": "./dist/translations/*"
  },
  "files": [
    "dist",
    "cdn"
  ],
  "keywords": [
    "web components",
    "custom elements",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agence-adeliom/adeliom-webcomponents.git"
  },
  "bugs": {
    "url": "https://github.com/agence-adeliom/adeliom-webcomponents/issues"
  },
  "scripts": {
    "create": "plop --plopfile scripts/plop/plopfile.js",
    "start": "node ./scripts/storybook.js",
    "build": "node ./scripts/build.js",
    "verify": "npm run prettier:check && npm run lint:check",
    "fix": "npm run prettier:fix && npm run lint:fix",
    "prettier:check": "prettier --check --log-level=warn .",
    "prettier:fix": "prettier --write --log-level=warn .",
    "lint:check": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --max-warnings 0 --fix",
    "test": "web-test-runner --group default",
    "test:watch": "web-test-runner -- --watch --group default",
    "test:component": "web-test-runner -- --watch --group",
    "ts:check": "tsc --noEmit --project ./tsconfig.json",
    "ts:build": "tsc --project ./tsconfig.prod.json --outdir dist",
    "dependencies:list": "npm-check-updates --format repo --peer",
    "dependencies:update": "npm-check-updates --peer -u && npm install",
    "prepare": "husky install",
    "storybook": "node ./scripts/storybook.js",
    "storybook:build": "node ./scripts/storybook.js --build",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.0.2",
    "@extractus/oembed-extractor": "^4.0.1",
    "@floating-ui/dom": "^1.5.3",
    "@lit/react": "^1.0.2",
    "@shoelace-style/animations": "^1.1.0",
    "@shoelace-style/localize": "^3.1.2",
    "composed-offset-position": "^0.0.4",
    "leaflet": "^1.9.4",
    "leaflet.gridlayer.googlemutant": "^0.14.0",
    "leaflet.locatecontrol": "^0.79.0",
    "leaflet.markercluster": "^1.5.3",
    "lit": "^3.1.0",
    "pretty-bytes": "^6.1.1",
    "qr-creator": "^1.0.0",
    "swiper": "^11.0.5",
    "unpic": "^3.16.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.9.0",
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@open-wc/testing": "^4.0.0",
    "@prettier/sync": "^0.3.0",
    "@rollup/plugin-replace": "^5.0.5",
    "@storybook/addon-a11y": "^7.6.12",
    "@storybook/addon-docs": "^7.6.12",
    "@storybook/addon-essentials": "^7.6.12",
    "@storybook/addon-interactions": "^7.6.12",
    "@storybook/addon-links": "^7.6.12",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.6.12",
    "@storybook/theming": "^7.6.12",
    "@storybook/web-components": "^7.6.12",
    "@storybook/web-components-vite": "^7.6.12",
    "@types/geojson": "^7946.0.13",
    "@types/leaflet": "^1.9.8",
    "@types/leaflet.gridlayer.googlemutant": "^0.4.9",
    "@types/leaflet.locatecontrol": "^0.74.4",
    "@types/leaflet.markercluster": "^1.5.4",
    "@types/mocha": "^10.0.6",
    "@types/react": "^18.2.43",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@web/dev-server-esbuild": "^1.0.1",
    "@web/test-runner": "^0.18.0",
    "@web/test-runner-commands": "^0.9.0",
    "@web/test-runner-playwright": "^0.11.0",
    "bootstrap-icons": "^1.11.2",
    "change-case": "^5.3.0",
    "command-line-args": "^5.2.1",
    "comment-parser": "^1.4.1",
    "custom-element-jet-brains-integration": "^1.4.0",
    "custom-element-vs-code-integration": "^1.2.1",
    "custom-element-vuejs-integration": "^1.0.0",
    "esbuild": "^0.19.9",
    "esbuild-plugin-replace": "^1.4.0",
    "escape-string-regexp": "^5.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-lit": "^1.10.1",
    "eslint-plugin-lit-a11y": "^4.1.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-wc": "^2.0.4",
    "esthetic": "^0.6.4-beta.1",
    "front-matter": "^4.0.2",
    "globby": "^14.0.0",
    "husky": "^8.0.3",
    "js-beautify": "^1.14.11",
    "lint-staged": "^15.2.0",
    "npm-check-updates": "^16.14.11",
    "plop": "^4.0.0",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "recursive-copy": "^2.0.14",
    "sinon": "^17.0.1",
    "storybook": "^7.6.12",
    "strip-css-comments": "^5.0.0",
    "terser": "^5.26.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "unist-util-visit": "^5.0.0",
    "user-agent-data-types": "^0.4.2",
    "vite": "^5.0.8"
  },
  "peerDependencies": {
    "@lit-labs/react": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "{src,scripts}/**/*.{js,ts}": [
      "eslint --max-warnings 0 --fix"
    ],
    "*": [
      "prettier --write --ignore-unknown --fix"
    ]
  }
}
